{
    "name": "dachcom-digital/seo",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore SEO Enrichment Bundle",
    "keywords": ["pimcore", "seo"],
    "homepage": "https://github.com/dachcom-digital/pimcore-seo",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "SeoBundle\\": "src/SeoBundle"
        }
